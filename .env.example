# Tee Sniper API Configuration
# Copy this file to .env and fill in the values

# Required: Shared secret for credential encryption (use a strong random string)
TSA_SHARED_SECRET=your-shared-secret-here

# Required: Base URL of the golf course booking site
TSA_BASE_URL=https://your-golf-course.example.com/

# Optional: Logging configuration
TSA_LOG_LEVEL=INFO
TSA_LOG_FORMAT=json

# Optional: Debug mode (set to true for development)
TSA_DEBUG=false
